{
  "mode": "parse",
  "debug": false,
  "probe_port": "9091",
  "proxy_port": "18000",
  "next": "127.0.0.1",
  "scan_interval_min": 5,
  "access_log_path": "/var/log/nginx/%s.access.log",
  "upstream_conf_path": "/etc/nginx/conf.d/003-proxies.conf",
  "max_history_log_each_ip": 1000,
  "max_cool_down_log_each_ip": 1000,
  "cool_down_min": 30,
  "dial_timeout": 15,
  "max_connection_timeout": 100,
  "strategy": "consecutive",
  "consecutive_failure": 3
}